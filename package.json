{
  "name": "@cs125/react-google-login",
  "description": "React support for Google client-side login.",
  "license": "MIT",
  "author": "Geoffrey Challen <geoffrey.challen@gmail.com>",
  "repository": "https://github.com/cs125-illinois/react-google-login",
  "bugs": "https://github.com/cs125-illinois/react-google-login/issues",
  "version": "2020.4.0",
  "main": "dist/index.cjs.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && GIT_COMMIT=$(git show --no-patch --format=%h) rollup -c",
    "check": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs example,docs && depcheck example/ --ignore-dirs .cache,dist,docs --ignores react,@mdx-js/parcel-plugin-mdx,@types/*,typescript,@cs125/react-google-login",
    "eslint": "eslint --fix src/ example/ --max-warnings=0 --ext .js,.ts,.tsx",
    "ncu": "ncu",
    "prepublishOnly": "npm run check && npm run build",
    "start": "concurrently -n example,src --handle-input \"cd example && npm start\" \"npm run watch\"",
    "tsc": "tsc --noEmit && tsc --project example --noEmit",
    "prettier": "prettier --write src/ example/ .vscode/ *.json *.js .*.js *.md && prettier-package-json --write && prettier-package-json --write example/package.json",
    "watch": "rollup -c -w"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "concurrently": "^5.1.0",
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "npm-check-updates": "^4.1.2",
    "prettier": "^2.0.4",
    "prettier-package-json": "^2.1.3",
    "react": "^16.12.0",
    "rollup": "^2.6.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,js,md}": [
      "prettier --write"
    ],
    ".*.js": [
      "prettier --write"
    ],
    "src/**/*.ts?(x)": [
      "eslint --fix",
      "bash -c \"tsc --noEmit --project .\"",
      "bash -c \"npm run depcheck\""
    ],
    "example/**/*.ts?(x)": [
      "eslint --fix",
      "bash -c \"tsc --noEmit --project example\"",
      "bash -c \"npm run depcheck\""
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
