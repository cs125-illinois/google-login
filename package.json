{
  "name": "@cs125/react-google-login",
  "description": "React support for Google client-side login.",
  "license": "MIT",
  "author": "Geoffrey Challen <geoffrey.challen@gmail.com>",
  "repository": "https://github.com/cs125-illinois/react-google-login",
  "bugs": "https://github.com/cs125-illinois/react-google-login/issues",
  "version": "2020.5.1",
  "main": "dist/index.cjs.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rm -rf dist && GIT_COMMIT=$(git show --no-patch --format=%h) rollup -c",
    "build:docs": "rm -rf docs/* && NODE_ENV=production webpack --config example/webpack.prod.config.js && rm -rf docs/*.map docs/*.js",
    "check": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs dist,docs --ignores=@types/*,concurrently,webpack-*,@typescript-eslint/*,@hot-loader/react-dom,purgecss,@cs125/react-google-login",
    "eslint": "eslint --fix src/ example/ --max-warnings=0 --ext .js,.ts,.tsx",
    "example": "webpack-dev-server --config example/webpack.dev.config.js",
    "ncu": "ncu",
    "prepublishOnly": "npm run check && npm run build",
    "serve": "serve -l 1234 docs",
    "start": "concurrently -n example,src --handle-input \"npm run example\" \"npm run watch\"",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write src/ example/ .vscode/ *.json *.js .*.js *.md && prettier-package-json --write",
    "watch": "rollup -c -w"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@mdx-js/loader": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "depcheck": "^0.9.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-check-updates": "^6.0.1",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3",
    "prop-types": "^15.7.2",
    "purgecss": "^2.2.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-children-utilities": "^2.1.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-hot-loader": "^4.12.21",
    "react-syntax-highlighter": "^12.2.1",
    "rollup": "^2.10.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "serve": "^11.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,js,md}": [
      "prettier --write"
    ],
    ".*.js": [
      "prettier --write"
    ],
    "{src,example}/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "bash -c \"tsc --noEmit --project .\"",
      "bash -c \"npm run depcheck\""
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
