{
  "name": "@cs125/google-login",
  "private": "true",
  "version": "2019.12.1",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "rollup -c -w --no-strict",
    "build": "rollup -c --no-strict",
    "tsc:check": "tsc --noEmit && tsc --project example",
    "prettier": "prettier --check *.json example/*.json src/**/*.{ts,tsx} example/**/*.{ts,tsx}",
    "prettier:write": "prettier --write *.json example/*.json src/**/*.{ts,tsx} example/**/*.{ts,tsx}",
    "tslint": "tslint --project . && tslint --project example",
    "tslint:fix": "tslint --fix --project . && tslint --fix --project example",
    "depcheck": "depcheck --ignore-dirs example --ignores depcheck && depcheck example/ --ignore-dirs .cache --ignores gatsby-plugin-*,@types/*,typescript"
  },
  "devDependencies": {
    "@types/react": "^16.9.15",
    "depcheck": "^0.9.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "rollup": "^1.27.8",
    "rollup-plugin-typescript2": "^0.25.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run depcheck && npm run tsc:check && lint-staged"
    }
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.ts?(x)": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "example/**/*.ts?(x)": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
