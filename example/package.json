{
  "private": true,
  "name": "@cs125/react-google-login",
  "description": "React support for Google client-side login.",
  "version": "2020.6.0",
  "scripts": {
    "build": "gatsby build",
    "check": "npm run depcheck && npm run eslint && npm run tsc && npm run format",
    "clean": "gatsby clean",
    "depcheck": "depcheck --ignore-dirs .cache,public --ignores=ts-node,gatsby-plugin-*,@typescript-eslint/*,eslint-*,gatsby-source-*,gatsby-transformer-*,nodemon",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "eslint": "eslint --fix --max-warnings=0 --ext .js,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" && prettier-package-json --write",
    "ncu": "ncu",
    "serve": "gatsby serve",
    "start": "nodemon",
    "tsc": "tsc"
  },
  "dependencies": {
    "@cs125/gatsby-theme-cs125": "^2020.6.23",
    "@material-ui/core": "^4.10.1",
    "gatsby": "^2.23.1",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-typescript": "^2.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "depcheck": "^0.9.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^6.0.1",
    "prettier": "2.0.5",
    "prettier-package-json": "^2.1.3"
  },
  "nodemonConfig": {
    "watch": [
      "package.json",
      "package-lock.json",
      "gatsby-config.js",
      "gatsby-config.ts",
      "gatsby-node.ts",
      "gatsby-browser.ts"
    ],
    "exec": "gatsby develop"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
